{
  "name": "@skyui/ui",
  "version": "0.0.0",
  "description": "",
  "keywords": [
    "vue",
    "ui",
    "component library"
  ],
  "author": "EvanSky",
  "license": "MIT",
  "homepage": "https://github.com/pan52yu/sky-ui/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pan52yu/sky-ui.git"
  },
  "bugs": {
    "url": "https://github.com/pan52yu/sky-ui/issues"
  },
  "scripts": {
    "build:package": "vite build --mode package",
    "build:full": "vite build --mode full",
    "build:full-min": "vite build --mode full-min",
    "build": "pnpm run build:package && pnpm run build:full && pnpm run build:full-min"
  },
  "main": "./dist/skyui-ui.umd.js",
  "module": "./dist/skyui-ui.mjs",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/skyui-ui.umd.js",
      "import": "./dist/skyui-ui.mjs",
      "types": "./dist/src/index.d.ts"
    },
    "./*": "./*",
    "./style/*": "./dist/style/*"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "dependencies": {
    "@skyui/button": "workspace:^",
    "@skyui/config-provider": "workspace:^",
    "@skyui/input": "workspace:^",
    "@skyui/shared": "workspace:^",
    "@skyui/styles": "workspace:^"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  }
}
